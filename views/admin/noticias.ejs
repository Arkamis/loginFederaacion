<section class="container-fluid">
  <h3>Crear una noticia Nueva 
    <a id="createNew" data-toggle="collapse" data-target="#addNoticia" role="button" aria-controls="addNoticia" href="#" class="btn">
    <i class="fas fa-plus-circle noti-t3" data-toggle="tooltip" data-placement="right" title="Crear una noticia nueva"></i>
    </a>
  </h3>
  <div class="row bg-light">
    <div class="col-12">
      <div class="container shadow rounded my-4 p-4 collapse " id="addNoticia">
        <form action="/noticia/addNoticia" enctype="multipart/form-data" method="POST" >
          <fieldset class="form-group row">
            <legend>Encabezado</legend>
            <div class="col-12 mb-3">
              <div class="input-group">
                <div class="input-group-prepend">
                  <span class="input-group-text">Volanta</span>
                  <input type="text" name="volanta" class="form-control" placeholder="Volanta de la noticia" maxlength="30">
                </div>
              </div>
            </div>
            <div class="col-12 mb-2">
              <div class="input-group-lg">
                <div class="input-group-prepend">
                  <span class="input-group-text">Titulo</span>
                  <input type="text" name="titulo" class="form-control" placeholder="Titulo de la noticia" maxlength="60">
                </div>
              </div>
            </div>
            <div class="col-12 mb-4">
              <div class="input-group-sm">
                <div class="input-group-prepend">
                  <span class="input-group-text">Copete</span>
                  <textarea name="copete" class="form-control" placeholder="Copete de la noticia, una breve introduccion..." maxlength="150"></textarea>
                </div>
              </div>
            </div>
          </fieldset>
          <fieldset class="form-group row">
            <legend>Multimedia</legend>
            <div class="col-12 mb-3">
              <img src="#" alt="Inserta Una imagen para tu noticia..." id="imgNoti">  
            </div>
            <div class="col-12 epigrafe">
              <div class="input-group mb-4">
                <div class="input-group-prepend">
                  <span class="input-group-text">Ep√≠grafe</span>
                  <textarea type="text" name="epigrafe" class="form-control" placeholder="Describe la imagen..."></textarea>
                </div>
              </div>
            </div>
            <div class="col-12 mb-4">
              <div class="input-group">
                <div class="input-group-prepend">
                  <span class="input-group-text">Foto</span>
                  <input type="file" name="foto" class="form-control" placeholder="Buscar Imagenes" id="imgInput" accesskey="f"/>
                  <label for="imgInput" id="imgName" class="mx-2 px-1">Seleccionar fotografia...</label>
                  <a id="imgButton" class="btn btn-outline-dark"><i class="far fa-image"></i></a>
                </div>
              </div>
            </div>
          </fieldset>
          <fieldset class="form-group row">
            <legend>Contenido Restante</legend>
            <div class="col-12">
              <div class="input-group">
                <div class="input-group-prepend">
                  <span class="input-group-text">Cuerpo</span>
                  <textarea id="cuerpo" type="text" name="cuerpo" class="form-control" placeholder="Desarrolla tu noticia, de lo mas importante..."></textarea>
                </div>
              </div>
            </div>
          </fieldset>
          <div class="row justify-content-center">
            <div class="col-7 align-items-end">
              <button type="button" class="btn btn-outline-warning btn-sm my-auto">Limpiar</button>
              <button id="cancelNew" type="button" class="btn btn-outline-danger btn-sm " data-toggle="collapse" data-target="#addNoticia">Cancelar</button>
            </div>
            <div class="col-auto">
              <button type="submit" class="btn btn-success btn-lg">Crear Noticia</button>
            </div>            
          </div>
        </form>
      </div>
    </div>
  </div>
  <hr>
</section>
